{
    "name": "ahmetbarut/php-router",
    "type": "library",
    "license": "MIT",
    "description": "Php simple use router",
    "autoload": {
        "psr-4": {
            "ahmetbarut\\PhpRouter\\": "src/"
        },
        "files": [
            "src/Helper/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "App\\": "app/"
        }
    },
    "authors": [{
        "name": "Ahmet Barut",
        "email": "ahmetbarut588@gmail.com"
    }],
    "minimum-stability": "dev",
    "require-dev": {
        "symfony/var-dumper": "*",
        "phpunit/phpunit": "10.0.x-dev",
        "guzzlehttp/guzzle": "7.4.x-dev"
    },
    "require": {
        "symfony/http-foundation": "6.1.x-dev"
    }
}
